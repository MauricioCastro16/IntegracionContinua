		"test:e2e": "playwright test",
		"docker:build": "docker build -t sveltekit-app:latest .",
		"docker:run": "docker run -p 3000:3000 --name sveltekit-app-container sveltekit-app:latest",
		"docker:stop": "docker stop sveltekit-app-container || true",
		"docker:rm": "docker rm sveltekit-app-container || true",
